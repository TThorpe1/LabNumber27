<Window x:Class="LabNumber27.ForecastInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabNumber27"
        mc:Ignorable="d"
        Title="ForecastInfo" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="loadForecastInfo" Margin="20" Click="loadForecastInfo_Click">Load Forecast Info</Button>
            <TextBlock x:Name="latitudeText" Margin="20"/>
            <TextBlock x:Name="longitudeText" Margin="20"/>
        </StackPanel>
        
    </Grid>
</Window>
